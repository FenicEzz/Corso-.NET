@{
    ViewBag.Title = "Home Page";
}

<main>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                @for (int i = 1; i < 11; i++)
                {
                    <th scope="col">@i</th>
                }
            </tr>
        </thead>
        <tbody>
            @for (int i = 1; i < 11; i++)
            {

                <tr>
                    <th scope="row">@i</th>
                    @for (int j = 1; j < 11; j++)
                    {
                        int z = i * j;
                        <td>@z</td>
                    }
                </tr>
            }
        </tbody>
    </table>
</main>